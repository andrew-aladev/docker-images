+      AC_REQUIRE([gl_FUNC_LOG])
+      LOGL_LIBM="$LOG_LIBM"
+    else
+      if test $HAVE_LOGL = 0; then
+        AC_REQUIRE([gl_FUNC_FREXPL])
+        AC_REQUIRE([gl_FUNC_ISNANL])
+        AC_REQUIRE([gl_FUNC_FLOORL])
