+      [AC_LANG_PROGRAM(
+         [[#ifndef __NO_MATH_INLINES
+           # define __NO_MATH_INLINES 1 /* for glibc */
+           #endif
+           #include <math.h>
+           double (*funcptr) (double) = floor;
+           double x;]],
+         [[x = funcptr(x) + floor(x);]])],
+      [gl_cv_func_floor_libm=])
